<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:file">
         <command
               commandId="de.alichs.eclipse.oauth.sample"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="de.alichs.eclipse.oauth.sample"
            name="Start Authorization">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="de.alichs.eclipse.oauth.StartAuthorizationHandler"
            commandId="de.alichs.eclipse.oauth.sample">
      </handler>
   </extension>

</plugin>
